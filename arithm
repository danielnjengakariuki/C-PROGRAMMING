#include <iostream>
using namespace std;
// this program ask the user to enter the width and length of a rectangle and then calculates the area of the rectangle
int main() {
  int Length, Width, Area;
  cout << "Enter the length: "; // Type the length and press enter
  cin >> Length; // Get user input from the keyboard
  cout<< "Enter the width: "; // Type the length and press enter
  cin>> Width; // Get user input from the keyboard
  Area = Length * Width ; // Calculates the Area of the rectangle
  cout<< "The Area of the rectangle is: "<< Area;
return 0;
}
